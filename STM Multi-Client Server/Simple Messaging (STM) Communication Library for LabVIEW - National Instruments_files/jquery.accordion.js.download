$(document).ready(function(){var bodyHeight=$("body").height();var vwptHeight=$(window).height();if(vwptHeight>bodyHeight){var footer=$("div.footer").height();var top=($(document).height())-(footer*2);if(!(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent))){$("div.footer").css('top',top);}}
$('.panel-heading').click(function(e){e.preventDefault();$(this).parent('.panel').siblings().each(function(){$(this).children('.panel-heading').children('span').first().removeClass("glyphicon-minus").addClass("glyphicon-plus");});$(this).parent().siblings().removeClass('active').addClass("inactive");if(!$(this).parent().hasClass('active')){$(this).parent().removeClass("inactive").addClass("active");$(this).find("span").removeClass("glyphicon-plus").addClass("glyphicon-minus");}else{$(this).parent().removeClass('active').addClass("inactive");$(this).find("span").removeClass("glyphicon-minus").addClass("glyphicon-plus");}});});